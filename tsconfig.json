{
  "compilerOptions": {
    "module": "CommonJS",
    "lib": ["es2015"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": ".",
    "typeRoots": ["."],
    "outDir": "lib",
    "types": [],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "allowJs": true
  },
  "files": ["index.d.ts", "enums.js"],
  "exclude": ["lib", "node_modules"]
}
